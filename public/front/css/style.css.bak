    margin-left: auto;
    margin-right: auto;
    border: 1px solid #fff;
}
.loader{

    font-family: 'Oswald', sans-serif;

    font-size: 40px;

    line-height: 100px;

    width: 380px;

    height: 110px;

    margin: 30px auto 0;

